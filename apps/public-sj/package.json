{
  "name": "@housingbayarea/public-sj",
  "version": "0.2.14",
  "description": "Public web app for the City of San Jose using the Bloom affordable housing system",
  "main": "index.js",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "dev": "export $(cat .env | xargs) && next -p ${NEXTJS_PORT:-3000}",
    "build": "next build",
    "test": "concurrently \"yarn dev\" \"cypress open\"",
    "export": "next export",
    "start": "next start",
    "dev:listings": "cd ../../services/listings && yarn dev",
    "dev:all": "concurrently \"yarn dev:listings\" \"yarn dev\""
  },
  "dependencies": {
    "@bloom-housing/core": "^0.2.14",
    "@bloom-housing/ui-components": "^0.2.14",
    "@mdx-js/loader": "1.6.16",
    "@next/mdx": "9.5.2",
    "@zeit/next-sass": "^1.0.1",
    "axios": "^0.19.2",
    "dotenv": "^8.2.0",
    "moment": "^2.27.0",
    "nanoid": "^3.1.12",
    "next": "9.5.2",
    "next-plugin-custom-babel-config": "^1.0.2",
    "node-polyglot": "^2.4.0",
    "node-sass": "^4.14.1",
    "openapi-client-axios": "^3.6.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-hook-form": "^6.5.3",
    "react-inlinesvg": "^2.0.1"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "9.5.2",
    "@types/markdown-to-jsx": "^6.11.2",
    "@types/node": "^12.12.54",
    "@types/react": "^16.9.47",
    "concurrently": "^5.3.0",
    "cypress": "^4.12.1",
    "js-levenshtein": "^1.1.6",
    "next-transpile-modules": "^4.1.0",
    "sass-loader": "^9.0.3",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1"
  }
}
